    "dayjs": "^1.10.6",
    "echarts": "4.9.0",
    "element-ui": "^2.15.7",
    "file-saver": "2.0.4",
    "fuse.js": "6.4.3",
    "highlight.js": "9.18.5",
    "html2canvas": "^1.3.2",
    "js-base64": "^3.7.2",
