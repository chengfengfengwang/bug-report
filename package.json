    "chalk": "4.1.0",
    "connect": "3.6.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "md5": "^2.2.1",
    "prettier": "^2.5.1",
    "runjs": "4.4.2",
