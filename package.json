    "lodash.clonedeep": "^4.5.0",
    "nprogress": "0.2.0",
    "qrcode": "^1.4.4",
    "qs": "^6.10.1",
    "quill": "1.3.7",
    "screenfull": "5.0.2",
    "socket.io-client": "^2.2.0",
    "sortablejs": "^1.14.0",
    "svgaplayerweb": "^2.3.2",
    "vee-validate": "^2.1.4",
    "vue": "^2.7.2",
    "vue-clipboard2": "^0.3.1",
    "vue-count-to": "1.0.13",
    "vue-cropper": "0.5.5",
